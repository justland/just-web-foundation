{
	"private": true,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "turbo run build",
		"build:doc": "turbo run build-doc",
		"check": "biome check",
		"clean": "turbo run clean",
		"cov": "vitest run --coverage",
		"coverage": "turbo run coverage",
		"cs": "changeset",
		"css": "pnpm --filter ./libs/css",
		"depcheck": "turbo run depcheck",
		"docs": "pnpm --filter ./apps/docs",
		"nuke": "run-s nuke:pkgs nuke:root",
		"nuke:pkgs": "turbo run nuke",
		"nuke:root": "rimraf node_modules",
		"prepare": "husky",
		"release": "changeset publish",
		"sb": "turbo run sb",
		"size": "turbo run size",
		"test": "turbo run test",
		"types": "pnpm --filter ./libs/types",
		"verify": "turbo run check cov depcheck size",
		"version": "changeset version",
		"w": "vitest"
	},
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.29.2",
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@locofy/cli": "^0.0.81",
		"@repobuddy/biome": "^1.7.1",
		"@repobuddy/vitest": "^1.2.2",
		"@types/react": "^19.1.2",
		"@vitest/coverage-v8": "^3.1.3",
		"husky": "^9.1.7",
		"npm-run-all2": "^7.0.2",
		"pathe": "^2.0.3",
		"playwright": "latest",
		"rimraf": "~6.0.1",
		"turbo": "^2.5.1",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	},
	"pnpm": {
		"overrides": {
			"react-dom": "catalog:",
			"react": "catalog:"
		}
	},
	"packageManager": "pnpm@10.13.1"
}
